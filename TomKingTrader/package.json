{
  "name": "tom-king-trader",
  "version": "1.0.0",
  "description": "Complete automated trading system implementing Tom King's systematic methodology with TastyTrade API integration",
  "main": "src/index.js",
  "scripts": {
    "start": "node start.js",
    "dev": "NODE_ENV=development node start.js",
    "prod": "NODE_ENV=production node start.js",
    "server": "node src/app.js",
    "test": "node tests/masterTestRunner.js complete",
    "test-quick": "node tests/masterTestRunner.js quick",
    "test-demo": "node tests/test-runner.js",
    "test-account": "node tests/masterTestRunner.js categories account_size",
    "test-bp": "node tests/masterTestRunner.js categories bp_utilization",
    "test-positions": "node tests/masterTestRunner.js categories position_scenarios",
    "test-market": "node tests/masterTestRunner.js categories market_conditions",
    "test-timing": "node tests/masterTestRunner.js categories time_scenarios",
    "test-edge": "node tests/masterTestRunner.js categories edge_cases",
    "test-api": "node tests/masterTestRunner.js complete --api",
    "test-list": "node tests/masterTestRunner.js list",
    "test-legacy": "node tests/test-runner.js all",
    "install-deps": "npm install",
    "setup": "cp .env.example .env && echo 'Please configure your .env file'",
    "lint": "eslint src/",
    "docs": "jsdoc src/ -d docs/",
    "build": "node scripts/build.js"
  },
  "keywords": [
    "trading",
    "options",
    "futures",
    "tastytrade",
    "systematic-trading",
    "tom-king",
    "risk-management",
    "pattern-analysis",
    "0DTE",
    "strangle",
    "LT112"
  ],
  "author": "Tom King Trading Framework Implementation",
  "license": "MIT",
  "dependencies": {
    "@dxfeed/dxlink-api": "^0.4.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "isomorphic-ws": "^5.0.0",
    "lodash": "^4.17.21",
    "mathjs": "^14.6.0",
    "node-fetch": "^2.6.7",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^8.52.0",
    "jsdoc": "^4.0.2",
    "mocha": "^10.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tom-king-trader.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/tom-king-trader/issues"
  },
  "homepage": "https://github.com/your-username/tom-king-trader#readme"
}
